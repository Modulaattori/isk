<script>
var dispatcher = new WebSocketRails('<%=home_domain%>/websocket');
var replace_slideitem = function(data) {$.ajax({
	type: "GET",
	url: "<%=url_for :controller => :slides%>/" + data.id,
	dataType: 'script'
});};

slidelist = dispatcher.subscribe('slide');
slidelist.bind('update', replace_slideitem);


</script>